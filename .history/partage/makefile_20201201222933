CC     = gcc
CFLAGS = 
LIBS   = 
EXEC   = tunnel64d

all: $(EXEC)

tunnel64d: tunnel64d.o iftun.o extremite.o configuration.o
		$(CC) -o $@ $^ $(LIBS)

tunnel64d.o: tunnel64d.c tunnel64d.h iftun.h extremite.h configuration.h
		$(CC) -o $@ -c $< 

iftun.o: iftun.c iftun.h
		$(CC) -o $@ -c $< 

extremite.o: extremite.c extremite.h iftun.h
		$(CC) -o $@ -c $< 

configuration.o: configuration.c configuration.h
		$(CC) -o $@ -c $< 

clean:
		rm -rf *.o *.txt

mrpropre: clean
		rm -rf $(EXEC)
